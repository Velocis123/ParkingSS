<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.booking.parkingss">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.Login_User"></activity>
        <activity android:name=".Activity.Requests" />
        <activity android:name=".Activity.Register" />
        <activity android:name=".Activity.Login" />
        <activity android:name=".Activity.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Edit_profile" />
        <activity android:name=".Activity.Status" />
        <activity android:name=".Activity.Acitivity_Details" />
        <activity android:name=".Activity.Profile" />
        <activity android:name=".Activity.Income" />
        <activity android:name=".Activity.MainActivity" />
    </application>

</manifest>